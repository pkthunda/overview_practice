<h1>Time Entry</h1>

<table>
  <thead>
    <tr>
      <th>Duration Worked</th>
      <th>Date</th>
      <!-- <th>Time Entries</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_entries.each do |time_entry| %>
      <tr>
        <td><%= time_entry.duration_worked %></td>
        <td><%= time_entry.worked_on %></td>
        <td><br><%= button_to 'Show', time_entry, :method => "get" %></br></td>
        <td><%= button_to 'Edit', edit_time_entry_path(time_entry), :method => "get" %></td>
        <td><%= button_to 'Destroy', time_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
